User Function CNABRHLOTE()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Declaracao de Variaveis                                             ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
Local __cArea__ := GetArea()
Local _NumArqu := SuperGetMV("RH_BRADESC",,"0000")

STATIC cNUM_LOT_GPE := "0000"

If cNUM_LOT_GPE == "0000"
	_NumArqu := STRZERO(VAL(_NumArqu)+1,4)
	PutMv( "RH_BRADESC", _NumArqu )
	cNUM_LOT_GPE := _NumArqu
Endif

RestArea(__cArea__)

Return cNUM_LOT_GPE
