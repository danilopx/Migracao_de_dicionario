User Function CNABRH()

Local cRet	:= ""

If SRA->(FieldPos("RA_TIPOCC_")) # 0
	If SRA->RA_TIPOCC_ == "3"
		cRet	:= "10510"
	ElseIf SRA->RA_TIPOCC_ == "2"
		cRet	:= "07380"
	Else
		cRet	:= "07050"
	Endif
Else
	cRet	:= "07050"
Endif

Return(cRet)

